	L		2.0
	/R
	__ASSERT==	__ACCU 1,	1.5
	__ASSERT==	__ACCU 1,	DW#16#3FC00000
	__ASSERT==	__STW A1,	1
	__ASSERT==	__STW A0,	0
	__ASSERT==	__STW OV,	0
	__ASSERT==	__STW OS,	0


