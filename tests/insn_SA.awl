	// Start timer and let it expire
	CLR
	SA		T 10
	SET
	R		T 10
	U		T 10
	__ASSERT==	__STW VKE,	0
	SET
	L		S5T#100MS
	SA		T 10
	U		T 10
	__ASSERT==	__STW VKE,	1
	__SLEEP		101
	U		T 10
	__ASSERT==	__STW VKE,	0


	// Start timer and reset VKE before it expires
	CLR
	SA		T 10
	SET
	R		T 10
	U		T 10
	__ASSERT==	__STW VKE,	0
	SET
	L		S5T#100MS
	SA		T 10
	U		T 10
	__ASSERT==	__STW VKE,	1
	CLR
	SA		T 10
	U		T 10
	__ASSERT==	__STW VKE,	0
	__SLEEP		101
	U		T 10
	__ASSERT==	__STW VKE,	0


	// Reset signal
	CLR
	SA		T 10
	SET
	R		T 10
	U		T 10
	__ASSERT==	__STW VKE,	0
	SET
	L		S5T#100MS
	SA		T 10
	U		T 10
	__ASSERT==	__STW VKE,	1
	SET
	R		T 10
	U		T 10
	__ASSERT==	__STW VKE,	0
	__SLEEP		101
	U		T 10
	__ASSERT==	__STW VKE,	0
