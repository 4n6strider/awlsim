	// Start timer and let it expire
	CLR
	SS		T 10
	SET
	R		T 10
	U		T 10
	__ASSERT==	__STW VKE,	0
	SET
	L		S5T#100MS
	SS		T 10
	U		T 10
	__ASSERT==	__STW VKE,	0
	__SLEEP		101
	U		T 10
	__ASSERT==	__STW VKE,	1


	// Start timer and reset VKE before it expires
	CLR
	SS		T 10
	SET
	R		T 10
	U		T 10
	__ASSERT==	__STW VKE,	0
	SET
	L		S5T#100MS
	SS		T 10
	U		T 10
	__ASSERT==	__STW VKE,	0
	CLR
	SS		T 10
	U		T 10
	__ASSERT==	__STW VKE,	0
	__SLEEP		101
	U		T 10
	__ASSERT==	__STW VKE,	1


	// Reset signal
	CLR
	SS		T 10
	SET
	R		T 10
	U		T 10
	__ASSERT==	__STW VKE,	0
	SET
	L		S5T#100MS
	SS		T 10
	U		T 10
	__ASSERT==	__STW VKE,	0
	SET
	R		T 10
	U		T 10
	__ASSERT==	__STW VKE,	0
	__SLEEP		101
	U		T 10
	__ASSERT==	__STW VKE,	0
